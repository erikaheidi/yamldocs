{
  "name": "erikaheidi/yamldocs",
  "description": "Experimental YAML Reference Builder / Markdown Document Generator",
  "license": "MIT",
  "homepage": "https://github.com/erikaheidi/yamldocs",
  "keywords": ["documentation","yaml", "cli"],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Builders\\": "builders"
    }
  },
  "require": {
    "minicli/minicli": "^3.0",
    "minicli/command-help": "^0.1.0",
    "minicli/command-stencil": "^0.1.0",
    "symfony/yaml": "^6.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "scripts": {
    "test" : ["pest"]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "bin": [
    "bin/yamldocs"
  ]
}
